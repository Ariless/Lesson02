public class Test5 {

	public static void main(String[] args) {

		int[] x = new int[10];
		int[] y = new int[10];
		int j = 0;
		for (int i = 0; i < x.length; i++) {
			x[i] = i + 1;
		}
		for (int i = 0; i < x.length; i++) {
			System.out.println(x[i]);
		}
		for (int i = 0; i < x.length; i++) {
			if (x[i] % 3 > 0) {
				y[j] = x[i];
				System.out.println(y[j]);
				j = j + 1;
			}
		}
	}
}
